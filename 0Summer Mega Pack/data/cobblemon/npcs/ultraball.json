{
  "baseScale": 1.0,
  "hitbox": {
    "width": 0.5,
    "height": 0.5,
    "fixed": false
  },
  "presets": [
    "cobblemon:inanimate"
  ],
  "config": [
    {
      "variableName": "loot_function",

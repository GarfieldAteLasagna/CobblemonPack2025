      "defaultValue": true
    },
    {
      "variableName": "item_count",
      "displayName": "Item Stack Size",
      "description": "The amount of the item that should be granted to the player (If \"Use Loot Table\" is False)",
      "type": "NUMBER",
      "defaultValue": "1"
    }
  ],
  "names": [
    "Ultra Ball"
  ],
  "interaction": {
    "type": "custom_script",
    "script": "q.run_script('cobblemon:grant_loot')"
  }
}
